library("igraph")
library(tidyverse)
library (CINNA)
library(visNetwork)
library(linkcomm)
library(tidygraph)
library(qgraph)
library(arsenal)
kangData <- read.csv(file = "data.csv")
edgeCnt = nrow(kangData)
grphPlt <- vector(mode="character", 2*edgeCnt)
k = 1
for (i in 1:edgeCnt) {
grphPlt[k] = as.character(kangData$From[i])
grphPlt[k + 1] = as.character(kangData$To[i])
k = k + 2
}
nodeLst <- unique(grphPlt)
nodeCnt = length(nodeLst)
View(kangData)
